#1

fruits = ["apple", "banana", "cherry"]
for x in fruits:
  if x == "banana":
    continue
  print(x)
#2
for i in range(1, 6):
    if i % 2 == 0:
        continue
    print(i)
#3
animals = ["dog", "cat", "bird"]
for a in animals:
    if a == "cat":
        continue
    print(a)
#4
nums = [3, -1, 5, -2, 7]
for n in nums:
    if n < 0:
        continue
    print(n)